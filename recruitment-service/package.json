{
   "name": "recruitment-service",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "migration:gen:init": "pnpm typeorm migration:generate -d src/config/datasource.ts src/migrations/Init",
      "migration:gen:diff": "pnpm typeorm migration:generate -d src/config/datasource.ts",
      "migration:run": "pnpm typeorm migration:run -d src/config/datasource.ts",
      "migration:revert": "pnpm typeorm migration:revert -d src/config/datasource.ts",
      "migration:show": "pnpm typeorm migration:show -d src/config/datasource.ts",
      "migration:check": "pnpm typeorm migration:generate src/migrations/tmpCheck -d src/config/datasource.ts --check --pretty"
   },
   "dependencies": {
      "@getbrevo/brevo": "^2.5.0",
      "@google/generative-ai": "^0.24.1",
      "@nestjs/common": "^11.0.1",
      "@nestjs/config": "^4.0.2",
      "@nestjs/core": "^11.0.1",
      "@nestjs/microservices": "^11.1.5",
      "@nestjs/platform-express": "^11.0.1",
      "@nestjs/serve-static": "^5.0.3",
      "@nestjs/swagger": "^11.2.0",
      "@nestjs/typeorm": "^11.0.0",
      "@types/pdf-parse": "^1.1.5",
      "axios": "^1.11.0",
      "bullmq": "^5.58.0",
      "chrono-node": "^2.8.4",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.2",
      "dayjs": "^1.11.18",
      "dotenv": "^17.2.1",
      "fs-extra": "^11.3.1",
      "helmet": "^8.1.0",
      "joi": "^17.13.3",
      "kafkajs": "^2.2.4",
      "multer": "^2.0.2",
      "openai": "^5.12.2",
      "pdf-parse": "^1.1.1",
      "pg": "^8.16.3",
      "pgvector": "^0.2.1",
      "redis": "^5.6.1",
      "reflect-metadata": "^0.2.2",
      "rxjs": "^7.8.1",
      "sharp": "^0.34.4",
      "tesseract.js": "^6.0.1",
      "typeorm": "^0.3.25",
      "uuid": "^11.1.0"
   },
   "devDependencies": {
      "@eslint/eslintrc": "^3.2.0",
      "@eslint/js": "^9.18.0",
      "@nestjs/cli": "^11.0.0",
      "@nestjs/schematics": "^11.0.0",
      "@swc/cli": "^0.6.0",
      "@swc/core": "^1.10.7",
      "@types/express": "^5.0.0",
      "@types/multer": "^2.0.0",
      "@types/node": "^22.10.7",
      "@types/sharp": "^0.32.0",
      "eslint": "^9.18.0",
      "eslint-config-prettier": "^10.0.1",
      "eslint-plugin-prettier": "^5.2.2",
      "globals": "^16.0.0",
      "prettier": "^3.4.2",
      "source-map-support": "^0.5.21",
      "ts-loader": "^9.5.2",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.7.3",
      "typescript-eslint": "^8.20.0"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
